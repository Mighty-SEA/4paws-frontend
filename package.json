{
  "name": "studio-admin",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack -p 3100 -H 0.0.0.0",
    "dev:3000": "next dev --turbopack -H 0.0.0.0",
    "build": "pnpm lint:fix && next build",
    "build2": "next build",
    "build:fast": "cross-env NODE_OPTIONS=--max-old-space-size=4096 next build",
    "build:prod": "cross-env NODE_OPTIONS=--max-old-space-size=4096 next build",
    "build:clean": "pnpm clean && cross-env NODE_OPTIONS=--max-old-space-size=4096 next build",
    "build:standalone": "cross-env NODE_ENV=production BUILD_STANDALONE=true next build",
    "build:standalone:package": "pnpm build:standalone && node scripts/package-standalone.js",
    "build:standalone:zip": "pnpm build:standalone:package && echo 'Standalone package with ZIP created!'",
    "build:copy": "pnpm build:fast && node scripts/create-portable-build.js",
    "build:portable": "pnpm build:copy && echo 'Portable build created in portable-build/ folder'",
    "build:zip": "pnpm build:portable && echo 'Compressed archive created!'",
    "clean": "node -e \"require('fs').rmSync('.next', {recursive: true, force: true})\"",
    "clean:cache": "node -e \"require('fs').rmSync('.next/cache', {recursive: true, force: true})\"",
    "clean:all": "node -e \"require('fs').rmSync('.next', {recursive: true, force: true})\" && node -e \"require('fs').rmSync('out', {recursive: true, force: true})\"",
    "typecheck": "tsc --noEmit",
    "ci:build": "pnpm typecheck && pnpm lint && next build",
    "start": "next start -p 3100 -H 0.0.0.0",
    "start:standalone": "node start-standalone.js",
    "start:prod": "cross-env NODE_ENV=production PORT=3100 HOST=0.0.0.0 node start-standalone.js",
    "lint": "next lint",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "node -e \"try { require('husky') } catch (e) { if (e.code !== 'MODULE_NOT_FOUND') throw e }\"",
    "generate:presets": "node --loader ts-node/esm src/scripts/generate-theme-presets.ts"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hookform/resolvers": "^3.10.0",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@tanstack/react-query": "^5.87.1",
    "@tanstack/react-table": "^8.21.3",
    "archiver": "^7.0.1",
    "axios": "^1.11.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^3.6.0",
    "embla-carousel-react": "^8.6.0",
    "eslint-plugin-unicorn": "^56.0.1",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.453.0",
    "next": "^15.5.2",
    "next-themes": "^0.4.6",
    "radix-ui": "^1.4.3",
    "react": "^19.1.1",
    "react-day-picker": "^9.9.0",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.62.0",
    "react-resizable-panels": "^3.0.5",
    "recharts": "^2.15.4",
    "simple-icons": "^15.13.0",
    "socket.io-client": "^4.8.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "vaul": "^1.1.2",
    "xlsx": "^0.18.5",
    "zod": "^3.25.76",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.16.0",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/node": "^22.18.1",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/parser": "^8.26.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.35.0",
    "eslint-config-next": "^15.5.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-sonarjs": "^3.0.5",
    "eslint-plugin-unused-imports": "^4.2.0",
    "globals": "^15.15.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.2",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4.1.5",
    "ts-node": "^10.9.2",
    "tw-animate-css": "^1.3.8",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.42.0"
  }
}
